{
    "name": "activ8-developers/k-core",
    "version": "3.6.0",
    "description": "Simple and lightweight yet powerful PHP framework for writing REST APIs",
    "keywords": ["framework", "k", "PHP", "lightweight", "simple", "router", "rest", "api"],
    "homepage": "https://github.com/ACTIV8-Developers/K-Core",
    "authors": [
        {
        "name": "Milos Kajnaco",
        "email": "milos@activ8.rs"
        },
        {
        "name": "Vedran Milakovic",
        "email": "vedran@activ8.rs"
        }
    ],
    "license": "MIT",
    "require":{
        "php": ">=7.2.0",
        "pimple/pimple": "3.0.*@dev",
        "psr/container": "1.0.0",
        "psr/simple-cache": "^1.0@dev",
        "predis/predis": "v1.1.0",
        "swiftmailer/swiftmailer": "v6.1.2",
        "bshaffer/oauth2-server-php": "v1.11.1",
        "kajna/purli": "dev-master",
        "ext-redis": "*",
        "ext-pdo": "*",
        "ext-json": "*"
    },
    "suggest": {
        "nesbot/carbon": "A simple API extension for DateTime."
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Core\\": "Core/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    }
}
